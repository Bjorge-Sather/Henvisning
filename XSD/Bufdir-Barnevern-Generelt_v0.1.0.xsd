					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Kommunenummer">
				<xs:annotation>
					<xs:documentation>
                    4-sifret kommunenummer, avgiverkommune.
                </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="4"/>
						<xs:maxLength value="4"/>
						<xs:pattern value="\d{4}"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Kommunenavn" minOccurs="0">
